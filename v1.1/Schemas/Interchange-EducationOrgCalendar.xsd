<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/0110" targetNamespace="http://ed-fi.org/0110" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="Ed-Fi-Core.xsd"/>
	<xs:annotation>
		<xs:documentation>===== Education Organization Interchange Model  =====</xs:documentation>
	</xs:annotation>
	<xs:element name="InterchangeEducationOrgCalendar">
		<xs:annotation>
			<xs:documentation>This interchange is for school calendar information.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Session" type="Session"/>
				<xs:element name="GradingPeriod" type="GradingPeriod"/>
				<xs:element name="CalendarDate" type="CalendarDate"/>
				<xs:element name="AcademicWeek" type="AcademicWeek"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>
